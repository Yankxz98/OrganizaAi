# Configurações do App
EXPO_PUBLIC_APP_NAME=Roteirize
EXPO_PUBLIC_APP_VERSION=1.0.0

# Configurações de API
EXPO_PUBLIC_API_URL=https://api.roteirize.com
EXPO_PUBLIC_API_VERSION=v1

# Configurações de Analytics
EXPO_PUBLIC_ANALYTICS_ID=your-analytics-id

# Configurações de Cache
EXPO_PUBLIC_CACHE_TTL=3600
EXPO_PUBLIC_MAX_CACHE_SIZE=50

# Configurações de Performance
EXPO_PUBLIC_ENABLE_LOGGING=false
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=true

# Configurações de Segurança
EXPO_PUBLIC_ENCRYPTION_KEY=your-encryption-key
EXPO_PUBLIC_ENABLE_SSL_PINNING=true 